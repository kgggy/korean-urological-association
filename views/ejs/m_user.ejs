

	<div class="board_list">
		<table>
			<colgroup>
				<col width="10%" />
				<col width="20%" />
				<col width="30%" />
				<col width="20%" />
				<col width="20%" />
			</colgroup>

			<thead>
				<tr>
					<th><input type="checkbox" name="ROW_CHK_ALL" id="ROW_CHK_ALL" /></th>
					<th>번호</th>
					<th>이메일</th>
					<th>닉네임</th>
                    <th>비고</th>
				</tr>
			</thead>
        <tbody>
				<% if(results.length > 0) { %>
					<% for(var i = 0; i < results.length; i++){ %>
						<tr>
							<td><input type="checkbox" name="ROW_CHK" class="ROW_CHK" IDX="<%=results[i].uid %>" /></td>
                            <td class="hand" onclick="window.open('orgm_viewForm','window_name','width=430,height=500,location=no,status=no,scrollbars=yes');"><%=results[i].uid%></td>
							<td>
								<%if(results[i].userEmail !=null && results[i].userEmail!='null' && results[i].userEmail!=''){%>
									<%=results[i].userEmail%>
								<%}%>
							</td>
							<td>
                                <%if(results[i].userNick !=null && results[i].userEmail!='null' && results[i].userEmail!=''){%>
								    <%= results[i].userNick %>
                                <%}%>
							</td>
                            <td>
                                <form action="m_user/userDelete" type="get">
                                    <input type="hidden" name="uid" value="<%=results[i].uid%>">
                                    <button type="submit">수정</button>
                                    <button type="submit">삭제</button>
                                    <button>등록</button>
                                </form>
                            </td>
						</tr>
					<% } %>
				<% } else {%>
					<tr>
						<td colspan="10">등록된 회원이 없습니다.</td>
					</tr>
				<% } %>
			</tbody>
		</table>
	</div>